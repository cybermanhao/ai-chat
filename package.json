{
  "name": "zz-ai-chat",
  "version": "1.0.0",
  "private": true,
  "workspaces": [
    "web",
    "electron",
    "iframe",
    "api",
    "mcp-server"
  ],
  "scripts": {
    "generate:avatars": "node --experimental-specifier-resolution=node --no-warnings scripts/generate-avatars.js",
    "dev:web": "pnpm run generate:avatars && vite --config web/vite.config.ts",
    "build:web": "pnpm run generate:avatars && tsc -b web && vite build --config web/vite.config.ts",
    "lint:web": "eslint web",
    "preview:web": "vite preview --config web/vite.config.ts",
    "test:web": "cd web && vitest --config vitest.config.ts",
    "test:web:coverage": "cd web && vitest run --coverage --config vitest.config.ts",
    "test:web:ui": "cd web && vitest --ui --config vitest.config.ts",
    "test": "pnpm run test:engine && pnpm run test:web",
    "build:engine": "pnpm tsc -b engine",
    "test:engine": "pnpm vitest run engine/tests",
    "test:pretty": "node scripts/test-runner.js",
    "build:mcp-node": "tsc -b mcp-node",
    "start:mcp-node": "pnpm run build:mcp-node && node mcp-node/dist/server.js",
    "test-server:mcp-node": "pnpm run build:mcp-node && node mcp-node/dist/test-server.js",
    "dev:mcp-node": "nodemon --watch mcp-node/src/**/*.ts --watch mcp-node/src/**/*.json --exec \"pnpm run build:mcp-node && node mcp-node/dist/server.js\"",
    "weather-server": "node --loader ts-node/esm mcp-node/src/weather-server.ts",
    "weather-test": "node --loader ts-node/esm mcp-node/src/test-mcp-service.ts",
    "weather-server:js": "pnpm tsc -p mcp-node && node mcp-node/dist/weather-server.js",
    "weather-test:js": "pnpm tsc -p mcp-node && node mcp-node/dist/test-mcp-service.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@ant-design/icons": "^6.0.0",
    "@ant-design/v5-patch-for-react-19": "^1.0.3",
    "@anthropic-ai/sdk": "^0.54.0",
    "@lobehub/icons": "^2.4.0",
    "@modelcontextprotocol/sdk": "^1.13.0",
    "@types/highlight.js": "^10.1.0",
    "@types/prismjs": "^1.26.5",
    "antd": "^5.26.1",
    "dotenv": "^16.5.0",
    "express": "^5.1.0",
    "highlight.js": "^11.11.1",
    "markdown-it": "^14.1.0",
    "node-fetch": "^3.3.2",
    "normalize.css": "^8.0.1",
    "openai": "^5.5.0",
    "prismjs": "^1.30.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.6.2",
    "scroll-into-view-if-needed": "^3.1.0",
    "uuid": "^11.1.0",
    "zod": "^3.25.67",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@testing-library/react": "^16.3.0",
    "@types/express": "^5.0.3",
    "@types/markdown-it": "^14.1.2",
    "@types/node": "^24.0.3",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vitejs/plugin-react": "^4.4.1",
    "@vitest/coverage-c8": "^0.33.0",
    "@vitest/ui": "^3.2.4",
    "chalk": "^5.4.1",
    "concurrently": "^9.2.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "esm": "^3.2.25",
    "eventsource": "^4.0.0",
    "globals": "^16.0.0",
    "happy-dom": "^18.0.1",
    "jsdom": "^26.1.0",
    "less": "^4.3.0",
    "nodemon": "^3.1.10",
    "ora": "^8.2.0",
    "prettier": "^2.8.8",
    "sharp": "^0.34.2",
    "ts-node": "^10.9.2",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5",
    "vitest": "^3.2.4"
  },
  "type": "module"
}
